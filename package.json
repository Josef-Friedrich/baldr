{
  "name": "baldr-songbook",
  "productName": "baldr-songbook",
  "description": "Meta package used for testing and documentation purposes.",
  "repository": {
    "type": "git",
    "url": "git@github.com:Josef-Friedrich/baldr-songbook.git"
  },
  "scripts": {
    "doc": "rm -f docs/*.html; jsdoc --configure ./docs/jsdoc-config.json --destination ./docs/",
    "doc-show": "xdg-open ./docs/index.html",
    "test": "mocha --reporter spec",
    "standard": "standard *.js packages/**/*.js test/*.js"
  },
  "author": {
    "name": "Josef Friedrich",
    "email": "josef@friedrich.rocks",
    "url": "https://josef.friedrich.rocks"
  },
  "license": "MIT",
  "homepage": "https://github.com/Josef-Friedrich/baldr-songbook",
  "devDependencies": {
    "@bldr/songbook-base": "*",
    "@bldr/songbook-cli": "*",
    "@bldr/songbook-electron-app": "*",
    "@bldr/songbook-intermediate-files": "*",
    "jsdoc": "^3.5.5",
    "lerna": "^3.8.0",
    "mocha-standard": "^1.0.0",
    "mocha": "^4.0.1",
    "rewire": "^4.0.1",
    "sinon": "^4.0.0",
    "spectron": "^3.7.2",
    "standard": "^12.0.1",
    "tmp": "0.0.33"
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ]
  }
}
