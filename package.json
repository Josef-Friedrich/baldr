{
  "name": "baldr-document-camera",
  "productName": "baldr-document-camera",
  "description": "A fullscreen electron app to display the video output from document cameras.",
  "version": "0.0.3",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:JosefFriedrich-nodejs/baldr-document-camera.git"
  },
  "scripts": {
    "build": "electron-packager ./ --out=dist --app-version=$npm_package_version --prune --asar --overwrite --arch=x64 --icon=./icon.icns",
    "doc": "jsdoc --destination ./docs/ .",
    "electron-rebuild": "electron-rebuild",
    "postinstall": "electron-rebuild",
    "start": "electron .",
    "test": "mocha"
  },
  "author": {
    "name": "Josef Friedrich",
    "email": "josef@friedrich.rocks",
    "url": "https://josef.friedrich.rocks"
  },
  "license": "MIT",
  "homepage": "https://github.com/JosefFriedrich-nodejs/baldr-document-camera",
  "devDependencies": {
    "electron-packager": "^9.1.0",
    "electron-rebuild": "^1.6.0",
    "electron": "^1.7.9",
    "jsdoc": "^3.5.5",
    "mocha-jshint": "^2.3.1",
    "mocha": "^4.0.1",
    "spectron": "^3.7.2"
  }
}
