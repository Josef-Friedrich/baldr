{
  "name": "baldr",
  "version": "0.0.1",
  "description": "Presentation software for music lessons",
  "main": "main.js",
  "scripts": {
    "build": "electron-packager . --out=dist --app-version=$npm_package_version --asar --prune --overwrite --arch=x64 --icon=./icon.icns",
    "doc-show": "xdg-open ./docs/index.html",
    "doc": "jsdoc --destination ./docs/ .",
    "electron-rebuild": "electron-rebuild",
    "postinstall": "electron-rebuild",
    "start": "electron .",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JosefFriedrich-nodejs/baldr.git"
  },
  "author": "Josef Friedrich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JosefFriedrich-nodejs/baldr/issues"
  },
  "homepage": "https://github.com/JosefFriedrich-nodejs/baldr#readme",
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-packager": "^9.1.0",
    "electron-rebuild": "^1.6.0",
    "jsdoc": "^3.5.5",
    "jsdom": "^11.3.0",
    "jshint": "^2.9.5",
    "mocha": "^4.0.1",
    "mocha-jshint": "^2.3.1",
    "rewire": "^2.5.2",
    "spectron": "^3.7.2"
  },
  "dependencies": {
    "js-yaml": "^3.10.0",
    "mousetrap": "^1.6.1",
    "pug": "^2.0.0-rc.4"
  }
}
