{
  "name": "baldr",
  "version": "0.1.27",
  "description": "A try to write my presentations for school in HTML5, CSS3 and Javascript using Electron. Virtual top level package for tooling support.",
  "homepage": "https://github.com/Josef-Friedrich/baldr#readme",
  "bugs": {
    "url": "https://github.com/Josef-Friedrich/baldr/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Josef-Friedrich/baldr.git"
  },
  "license": "MIT",
  "author": {
    "name": "Josef Friedrich",
    "email": "josef@friedrich.rocks",
    "url": "https://josef.friedrich.rocks"
  },
  "scripts": {
    "build": "electron-builder",
    "clean": "lerna clean; rm -rf dist; rm -rf src/electron-app/dist; rm -rf masters/songbook/packages/electron-app/dist; rm -rf masters/camera/electron-app/dist",
    "doc": "rm -f docs/*.html; jsdoc --configure ./docs/jsdoc-config.json --readme API.md .",
    "doc-show": "xdg-open ./docs/index.html",
    "pack": "electron-builder --dir",
    "spectron": "mocha --fgrep '#spectron' 'test/*.js' 'src/app/test/*.js' 'src/lib/test/*.js' 'masters/audio/test/*.js' 'masters/camera/test/*.js' 'masters/editor/test/*.js' 'masters/image/test/*.js' 'masters/markdown/test/*.js' 'masters/person/test/*.js' 'masters/question/test/*.js' 'masters/quote/test/*.js' 'masters/svg/test/*.js' 'masters/website/test/*.js'",
    "standard": "standard",
    "start": "electron .",
    "test": "./run-mocha.js",
    "unittest": "mocha --fgrep '#unittest' 'test/*.js' 'src/app/test/*.js' 'src/lib/test/*.js' 'masters/audio/test/*.js' 'masters/camera/test/*.js' 'masters/editor/test/*.js' 'masters/image/test/*.js' 'masters/markdown/test/*.js' 'masters/person/test/*.js' 'masters/question/test/*.js' 'masters/quote/test/*.js' 'masters/svg/test/*.js' 'masters/website/test/*.js'"
  },
  "devDependencies": {
    "@bldr/camera-electron-app": "^0.0.21",
    "@bldr/core": "^0.0.16",
    "@bldr/electron-app": "^0.0.21",
    "@bldr/electron-apps-installer": "^0.0.15",
    "@bldr/foundation-master": "^0.0.16",
    "@bldr/master-audio": "^0.0.17",
    "@bldr/master-camera": "^0.0.17",
    "@bldr/master-editor": "^0.0.17",
    "@bldr/master-image": "^0.0.13",
    "@bldr/master-markdown": "^0.0.18",
    "@bldr/master-person": "^0.0.13",
    "@bldr/master-question": "^0.0.11",
    "@bldr/master-quote": "^0.0.11",
    "@bldr/master-svg": "^0.0.11",
    "@bldr/master-video": "^0.0.13",
    "@bldr/master-website": "^0.0.11",
    "@bldr/seating-plan": "^0.1.8",
    "@bldr/songbook-base": "^0.1.12",
    "@bldr/songbook-cli": "^0.1.16",
    "@bldr/songbook-electron-app": "^0.1.13",
    "@bldr/songbook-intermediate-files": "^0.1.15",
    "@bldr/test-helper": "^0.0.17",
    "@bldr/theme-default": "^0.0.16",
    "@bldr/theme-handwriting": "^0.0.16",
    "electron": "^5.0.7",
    "electron-builder": "^21.1.1",
    "electron-packager": "^14.0.2",
    "electron-rebuild": "^1.8.5",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.4",
    "js-object-pretty-print": "^0.3.0",
    "jsdoc": "^3.6.3",
    "jsdom": "^15.1.1",
    "lerna": "^3.16.1",
    "mocha": "^6.2.0",
    "mocha-standard": "^1.0.0",
    "rewire": "^4.0.1",
    "sinon": "^7.3.2",
    "spectron": "^7.0.0",
    "standard": "^13.0.2",
    "tmp": "0.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "build": {
    "appId": "rocks.friedrich.baldr",
    "mac": {
      "category": "public.app-category.education",
      "target": "dmg"
    },
    "linux": {
      "target": [
        "deb"
      ]
    },
    "fileAssociations": [
      {
        "ext": "baldr",
        "name": "BALDR Presentation Software",
        "description": "File format of the BALDR Presentation Software",
        "icon": "icon.icns",
        "role": "Viewer"
      }
    ]
  },
  "gitHead": "2eadc451ebbeaa981210b2d91360533a5ef0f009",
  "standard": {
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ],
    "ignore": [
      "node_modules",
      "docs",
      "out"
    ]
  }
}
