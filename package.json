{
  "name": "baldr-sbook",
  "productName": "baldr-sbook",
  "description": "A fullscreen electron app to display songs in a class room using a projector.",
  "version": "0.0.3",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:JosefFriedrich-nodejs/baldr-sbook.git"
  },
  "scripts": {
    "build": "electron-packager . --out=dist --app-version=$npm_package_version --asar --prune --overwrite --arch=x64 --icon=./icon.icns",
    "electron-rebuild": "electron-rebuild",
    "postinstall": "electron-rebuild",
    "start": "electron .",
    "test": "mocha"
  },
  "author": {
    "name": "Josef Friedrich",
    "email": "josef@friedrich.rocks",
    "url": "https://josef.friedrich.rocks"
  },
  "license": "MIT",
  "homepage": "https://github.com/JosefFriedrich-nodejs/baldr-sbook",
  "dependencies": {
    "mousetrap": "*",
    "jquery": "*",
    "selectize": "*"
  },
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-packager": "^9.1.0",
    "electron-rebuild": "^1.6.0",
    "mocha": "^4.0.1",
    "mocha-jshint": "^2.3.1",
    "spectron": "^3.7.2"
  }
}
