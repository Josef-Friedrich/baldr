{
    "opts": {
        "encoding": "utf8",
        "destination": ".",
        "recurse": true,
        "template": "node_modules/better-docs"
    },
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": "\\.(vue|js)$",
        "excludePattern": "(node_modules|dist|out|test|tests)"
    },
    "plugins": [
        "node_modules/jsdoc-vuejs",
        "plugins/markdown"
    ]
}

