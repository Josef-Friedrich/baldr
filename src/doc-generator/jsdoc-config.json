{
    "opts": {
        "encoding": "utf8",
        "destination": ".",
        "recurse": true,
        "template": "node_modules/better-docs"
    },
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": "\\.(vue|js)$"
    },
    "plugins": [
        "node_modules/better-docs/component",
        "node_modules/jsdoc-vuejs",
        "plugins/markdown"
    ]
}

