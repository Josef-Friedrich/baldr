{
  "name": "@bldr/songbook-electron-app",
  "version": "0.1.19",
  "description": "A fullscreen electron app to display songs in a class room using a projector.",
  "homepage": "https://github.com/Josef-Friedrich/baldr#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Josef-Friedrich/baldr.git"
  },
  "license": "MIT",
  "author": {
    "name": "Josef Friedrich",
    "email": "josef@friedrich.rocks",
    "url": "https://josef.friedrich.rocks"
  },
  "main": "electron-main.js",
  "scripts": {
    "build": "electron-packager . --out=dist --app-version=$npm_package_version --asar --prune --overwrite --arch=x64 --icon=./icon.icns",
    "start": "electron .",
    "dist": "electron-builder"
  },
  "dependencies": {
    "@bldr/songbook-base": "^0.1.18",
    "@bldr/songbook-core": "^0.1.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "mousetrap": "^1.6.3",
    "selectize": "^0.12.6"
  },
  "devDependencies": {
    "@electron-forge/maker-deb": "^6.0.0-beta.39",
    "electron": "^5.0.8",
    "electron-packager": "^14.0.3",
    "electron-prebuilt-compile": "4.0.0",
    "electron-rebuild": "^1.8.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "build": {
    "appId": "rocks.friedrich.baldr.songbook"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "baldr-songbook",
        "executableName": "baldr-songbook",
        "derefSymlinks": false,
        "out": "baldr-songbook"
      },
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "dmg"
        ],
        "linux": [
          "deb"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-deb"
        }
      ]
    }
  },
  "gitHead": "ed4f60e66c8f420711d3eb15185620f7688e6fc9"
}
