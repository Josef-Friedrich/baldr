{
    "opts": {
        "encoding": "utf8",
        "destination": ".",
        "recurse": true
    },
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": "\\.(js|vue)$",
        "excludePattern": "(^|\\/|\\\\)_",
        "exclude": ["node_modules"]
    },
    "plugins": [
        "jsdoc-vuejs"
    ]
}

