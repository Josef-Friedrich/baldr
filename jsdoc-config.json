{
    "opts": {
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true
    },
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "exclude": [
            "node_modules"
        ]
    }
}
